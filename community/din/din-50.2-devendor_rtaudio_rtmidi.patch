+
+	int num_channels;
+	int samples_per_channel;
+	int last_sample;
+
+  static int audio_wanted (void *ob, void *ib, unsigned int spc, double t, RtAudioStreamStatus status, void *data);
+
+
+};
+
+extern audio_out aout;
+extern int SAMPLE_RATE;
+extern float SAMPLE_DURATION;
+
+#endif
